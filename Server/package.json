{
  "name": "LittleHomebrewHelper",
  "version": "1.0.0",
  "description": "Basic campaign manager",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "electron .",
    "prestart": "npm run build",
    "package": "npm run build && electron-builder build",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src/**/*.ts --quiet --fix",
    "clean": "rimraf ./dist ./release"
  },
  "repository": "https://github.com/torphexi/LittleHomebrewHelper",
  "keywords": [
    "Electron",
    "typescript"
  ],
  "author": {
    "name": "torphexi"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^14.14.27",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "bootstrap": "^4.6.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "eslint": "^7.19.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/node": "^14.14.27",
